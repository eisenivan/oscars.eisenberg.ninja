{
  "rules": {
    "ballots": {
      "2021": {
        "$uid": {
          ".write": "root.child('settings/2021/locked').val() === false && $uid === auth.uid",
          ".read": "auth.uid !== null && ($uid === auth.uid || root.child('settings/2021/locked').val() === true)"
        },
        ".read": "auth.uid !== null && root.child('settings/2021/locked').val() === true"
      },
      "2022": {
        "$uid": {
          ".write": "root.child('settings/2022/locked').val() === false && $uid === auth.uid",
          ".read": "auth.uid !== null && ($uid === auth.uid || root.child('settings/2022/locked').val() === true)"
        },
        ".read": "auth.uid !== null && root.child('settings/2022/locked').val() === true"
      },
      "2023": {
        "$uid": {
          ".write": "root.child('settings/2023/locked').val() === false && $uid === auth.uid",
          ".read": "auth.uid !== null && ($uid === auth.uid || root.child('settings/2023/locked').val() === true)"
        },
        ".read": "auth.uid !== null && root.child('settings/2023/locked').val() === true"
      },
      "2024": {
        "$uid": {
          ".write": "root.child('settings/2024/locked').val() === false && $uid === auth.uid",
          ".read": "auth.uid !== null && ($uid === auth.uid || root.child('settings/2024/locked').val() === true)"
        },
        ".read": "auth.uid !== null && root.child('settings/2024/locked').val() === true"
      }
    },
    "scores": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "auth.uid !== null"
      }
    },
    "settings": {
      ".write": "root.child('admins').child(auth.uid).exists() === true",
      ".read": true
    },
    "groups": {
      "2021": {
        ".read": true,
        ".write": false
      },
      "2022": {
        ".read": true,
        ".write": false
      },
      "2023": {
        ".read": true,
        ".write": false
      },
      "2024": {
        ".read": true,
        ".write": false
      }
    }
  }
}
